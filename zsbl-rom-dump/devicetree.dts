			interrupt-parent = <0x4>;
			interrupts = <0x13 0x14 0x15 0x16>;
			next-level-cache = <0x5 0x6 0x7 0x8>;
			reg = <0x0 0x2010000 0x0 0x1000 0x0 0x8000000 0x0 0x200000>;
