/dts-v1/;

/ {
	#address-cells = <0x2>;
	#size-cells = <0x2>;
	compatible = "SiFive,FU740-C000-dev", "fu700-dev", "sifive-dev";
	model = "SiFive,FU740-C000";

	aliases {
		serial0 = "/soc/serial@10010000";
		serial1 = "/soc/serial@10011000";
