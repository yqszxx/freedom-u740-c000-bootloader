			i-cache-sets = <0x80>;
			i-cache-size = <0x4000>;
			next-level-cache = <0x1 0x2>;
			reg = <0x0>;
			riscv,isa = "rv64imac";
			sifive,dtim = <0x3>;
			status = "okay";
			timebase-frequency = <0xf4240>;

			interrupt-controller {
				#interrupt-cells = <0x1>;
				compatible = "riscv,cpu-intc";
				interrupt-controller;
				linux,phandle = <0x9>;
				phandle = <0x9>;
			};
		};

		cpu@1 {
			clock-frequency = <0x0>;
