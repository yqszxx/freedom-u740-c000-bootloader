			next-level-cache = <0x1 0x2>;
			reg = <0x1>;
			riscv,isa = "rv64imafdc";
			status = "okay";
			timebase-frequency = <0xf4240>;
			tlb-split;

			interrupt-controller {
				#interrupt-cells = <0x1>;
				compatible = "riscv,cpu-intc";
				interrupt-controller;
				linux,phandle = <0xa>;
				phandle = <0xa>;
			};
		};

		cpu@2 {
			clock-frequency = <0x0>;
			compatible = "sifive,bullet0", "riscv";
			d-cache-block-size = <0x40>;
